{
  "name": "@cmux/server",
  "version": "1.0.0",
  "description": "cmux server",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./server": {
      "import": "./src/server.ts",
      "types": "./src/server.ts"
    },
    "./socket-handlers": {
      "import": "./src/socket-handlers.ts",
      "types": "./src/socket-handlers.ts"
    },
    "./gitDiff": {
      "import": "./src/gitDiff.ts",
      "types": "./src/gitDiff.ts"
    },
    "./realtime": {
      "import": "./src/realtime.ts",
      "types": "./src/realtime.ts"
    },
    "./vscode/serveWeb": {
      "import": "./src/vscode/serveWeb.ts",
      "types": "./src/vscode/serveWeb.ts"
    },
    "./utils/fileLogger": {
      "import": "./src/utils/fileLogger.ts",
      "types": "./src/utils/fileLogger.ts"
    }
  },
  "type": "module",
  "scripts": {
    "start": "vite-node src/local-dev.ts",
    "dev3": "DENO_COMPAT=1 deno run -A --watch-hmr src/local-dev.ts",
    "devtsx": "bunx tsx src/local-dev.ts",
    "dev": "bun run --watch src/local-dev.ts",
    "dev2": "vite-node src/local-dev.ts",
    "dev0": "DENO_COMPAT=1 deno run -A src/local-dev.ts",
    "dev-vite": "vite-node --watch src/local-dev.ts",
    "start-deno": "deno run -A src/local-dev.ts",
    "dev-deno": "deno run -A --watch-hmr src/local-dev.ts",
    "start-bun": "bun run src/local-dev.ts",
    "dev-bun": "bun run --hot src/local-dev.ts",
    "daytona": "bun x vite-node scripts/hello-daytona.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.6",
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/openai": "^2.0.19",
    "@cmux/convex": "workspace:*",
    "@cmux/shared": "workspace:*",
    "@cmux/www-openapi-client": "workspace:*",
    "@daytonaio/sdk": "^0.22.1",
    "@octokit/auth-app": "^6.1.4",
    "@t3-oss/env-core": "^0.13.8",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/headless": "^5.5.0",
    "ai": "^5.0.22",
    "ansi-escape-sequences": "^6.2.4",
    "archiver": "^7.0.1",
    "chokidar": "^4.0.3",
    "convex": "1.27.1",
    "dockerode": "^4.0.2",
    "dotenv": "^16.6.1",
    "electron": "^38.2.2",
    "express": "^4.21.2",
    "fuzzysort": "^3.1.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "httpxy": "^0.1.7",
    "ignore": "^7.0.5",
    "minimatch": "^10.0.3",
    "octokit": "^3.2.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "tar": "^7.4.3",
    "vite-node": "^3.2.4",
    "ws": "^8.18.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bun": "^1.3.0",
    "@types/dockerode": "^3.3.31",
    "@types/express": "^4.17.23",
    "@types/http-proxy": "^1.17.16",
    "@types/node": "^24.9.1",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "vite": "^7.0.5",
    "vitest": "3.2.4"
  }
}
