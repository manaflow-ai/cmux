{
  "name": "@cmux/www",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env -- next dev --turbo --port 9779",
    "prebuild": "bun run generate-openapi-client",
    "build": "./scripts/build.sh",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "dotenv -e ../../.env -- vitest run",
    "generate-openapi-client": "bun run --env-file ../../.env scripts/watch-openapi.ts",
    "generate-openapi-client:watch": "bun run --watch --env-file ../../.env scripts/watch-openapi.ts",
    "backfill-pr": "bun run --env-file ../../.env scripts/backfill-pr.ts",
    "backfill-repo": "bun run --env-file ../../.env scripts/backfill-repo.ts"
  },
