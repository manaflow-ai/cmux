    "@cmux/shared": "workspace:*",
    "@cmux/www-openapi-client": "workspace:*",
    "@convex-dev/react-query": "^0.0.0-alpha.11",
    "@hono/node-server": "^1.18.1",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
